{
    "manifestVersion": 1,
    "id": "mover-card-onprem",
    "publisher": "JeffersonMota",
    "version": "0.0.1",
    "name": "Mover Card",
    "description": "Mover cards entre times e colunas",
    "public": true,
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.work_write",
        "vso.project_write",
        "vso.settings"
    ],
    "files": [
        { "path": "index.html", "addressable": true },
        { "path": "scripts", "addressable": true },
        { "path": "css", "addressable": true },
        { "path": "sdk", "addressable": true },
        { "path": "icon.png", "addressable": true }
    ],
    "icons": {
        "default": "icon.png"
    },
    "contributions": [
        {
            "id": "move-card-form-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Move card UI",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Mover Card",
                "uri": "index.html",
                "height": 180
            }
        }
    ]
}